
@{
    ViewBag.Title = "AnswerPage";
}

<h2>AnswerPage</h2>

<div class="form-horizontal" style="display:inline-block;">
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    
    <span class="label label-default" style="font-size:28px;"> You Chose @ViewBag.QA </span>
</div>
<span class="label label-default" style="font-size:28px;"> @ViewBag.AP AP</span>
<span class="label label-default" style="font-size:28px;"> @ViewBag.SP SP</span>
<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <br /><br />
        <input type="submit" value="Submit" name="Submit" class="btn btn-default" />
        <input type="submit" value="reset" name="reset" class="btn btn-default" />
    </div>
</div>